{
  "name": "react-frontend-template",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^16.11.19",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "axios": "^0.24.0",
    "env-cmd": "^10.1.0",
    "node-sass": "^7.0.1",
    "react": "^17.0.2",
    "react-clear-cache": "^1.4.12",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1",
    "react-scripts": "5.0.0",
    "typescript": "^4.5.4",
    "web-vitals": "^2.1.3"
  },
  "scripts": {
    "start": "env-cmd -e local react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build": "react-scripts build",
    "generate-build-meta": "./node_modules/react-clear-cache/bin/cli.js",
    "prebuild": "npm run generate-build-meta",
    "build:dev": "env-cmd -e dev npm run build",
    "build:qa": "env-cmd -e qa npm run build",
    "build:prod": "GENERATE_SOURCEMAP=false env-cmd -e prod npm run build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/node-sass": "^4.11.2",
    "@types/react-router-dom": "^5.3.2"
  }
}
